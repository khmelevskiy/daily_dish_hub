{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^24.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run copy-build",
    "build:ci": "vite build",
    "test": "vitest",
    "preview": "vite preview",
    "copy-build": "rm -rf ../app/static/assets && mkdir -p ../app/static && cp dist/index.html ../app/static/ && cp -r dist/assets ../app/static/"
  },
  "eslintConfig": {
    "rules": {
      "react/no-unescaped-entities": "off",
      "no-empty": ["error", { "allowEmptyCatch": true }]
    }
  }
}
